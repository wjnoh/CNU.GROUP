<%= render "templates/navbar" %>

<div class="wrapper">
  <!-- 메인사진 -->
  <%= render "board/templates/header" %>

  <div class="container">
    <div class="row">
      <!-- 좌측 영역 -->
      <%= render "board/templates/groupNav" %>

      <!-- 우측 영역 -->
      <% unless @belongsToMember  %>
        <%= render "board/templates/stream/infoPost" %>
      <% else %>
        <%= render "board/templates/posts" %>
      <% end %>
    </div>
  </div>
</div>

<%= render "templates/footer" %>


<!-- JS 코드  -->
<%= render "board/templates/postsJS" %>
